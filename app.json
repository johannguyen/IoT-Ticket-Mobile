{
  "expo": {
    "name": "IoT-Ticket Mobile App",
    "slug": "IoT-Ticket-Mobile-App",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "android": {
      "package": "com.wapice.IoTTicketMobile"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.wapice.IoTTicketMobile",
      "supportsTablet": true
    }
  }
}